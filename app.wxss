.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.bottomShadow{
  position: fixed;
  bottom: 0;
  height: 0rpx;
  width: 100%;
  z-index: 100;
  box-shadow: 0px 5rpx 20rpx 5rpx rgba(0, 0, 0, 0.2) ;
}
